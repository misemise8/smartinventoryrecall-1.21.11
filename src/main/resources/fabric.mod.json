{
	"schemaVersion": 1,
	"id": "smartinventoryrecall",
	"version": "${version}",
	"name": "Smart Inventory Recall",
	"description": "Automatically saves your inventory layout when you die and restores items to their original slots when picked up. Items are compacted near your death location for easy recovery.",
	"authors": [
		"Misemise"
	],
	"contact": {
		"homepage": "https://github.com/Misemise8/SmartInventoryRecall",
		"sources": "https://github.com/Misemise8/SmartInventoryRecall"
	},
	"license": "MIT",
	"icon": "assets/smartinventoryrecall/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.SmartInventoryRecall.SmartInventoryRecall"
		],
		"fabric-datagen": [
			"net.SmartInventoryRecall.SmartInventoryRecallDataGenerator"
		]
	},
	"mixins": [
		"smartinventoryrecall.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}